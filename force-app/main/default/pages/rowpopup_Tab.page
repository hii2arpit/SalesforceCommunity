<apex:page controller="dynamicControllerpopup" standardStyleSheets="false" > 

   <apex:form >
    
    <apex:outputPanel id="test">
        <apex:outputPanel rendered="{!first}">
        <apex:commandbutton value="one" action="{!test}" rerender="tabrender,test" style="width:150pt; height:150%;background:#D3D3D3;border:1px solid #a8a8a8;text-align:left;"/>
        <apex:commandbutton value="Two" action="{!test1}" rerender="tabrender,test" style="width:150pt; height:150%;background:white;border:1px solid #a8a8a8;text-align:left;"/>
        </apex:outputPanel>
        
        <apex:outputPanel rendered="{!second}">
        <apex:commandbutton value="one" action="{!test}" rerender="tabrender,test" style="width:150pt; height:150%;background:white;border:1px solid #a8a8a8;text-align:left;"/>
